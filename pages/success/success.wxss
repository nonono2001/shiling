/* pages/register/register.wxss */
.bg5{
   background: url('../../images/bg_5.jpg') no-repeat center center /cover;
}
.box2{
  margin-top: 225rpx;
}
    .box .box_top .title{
      padding-top:14rpx;
      text-align: center;
      margin-right:67rpx;
      font-size:24rpx;
      letter-spacing: 12rpx;
    }
  .box .box_con{
    text-align: center;
    clear: both;
    overflow: hidden;
    padding-bottom:30rpx;
  }
    .box .box_con .text_1{
      color:#cea869;
      font-size:36rpx;
      border-bottom:1rpx solid #cea869;
      width:390rpx;
      margin: 0 auto;
      padding-bottom:20rpx;
    }

    .box .box_con .text_2{
      color:#cea869;
      font-size:34rpx;
      border-bottom:1rpx solid #cea869;
      width:390rpx;
      margin: 0 auto;
      padding-bottom:20rpx;
      padding-top:10rpx;
      line-height: 44rpx;
      margin-bottom: 40rpx;
    }
      
      .box .box_con .img_box{
        width:410rpx;
        padding-bottom:30rpx;
        text-align: center;
        
      }
      .box .img_left{
        float:left;
        margin-left:100rpx;
      }

      .box .img_right{
        float:right;
      }
      .box .box_con .img_box image{
        width:122rpx;
        padding:0;
        margin:0;
        height:122rpx;
      }
      .box .box_con .img_text{
        color: #c3caff;
        font-size:24rpx;
      }
      .box .btn_box button{
        color:#cea869;
        background-color: #0b1341;
        text-align: center;
        display: block;
        width: 220rpx;
        line-height: 48rpx;
        border: 1px solid #856e56;
        border-radius: 20rpx;
        font-size:26rpx;
       
      }
